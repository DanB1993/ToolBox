{
    "name": "danbaker/toolbox",
    "description": "A PHP toolbox providing useful helpers and utilities.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DanBaker\\ToolBox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DanBaker\\ToolBox\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}